{
  "name": "palabs/enum-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle for palabs/php-enum",
  "keywords": ["enum", "php-enum", "symfony"],
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "palabs/php-enum": "~0.2.1",
    "doctrine/common": "^2.8",
    "doctrine/orm": "^2.6 || ^2.7",
    "symfony/form": "^3.0 || ^4.0 || ^5.0",
    "symfony/doctrine-bridge": "^3.0 || ^4.0 || ^5.0",
    "symfony/translation-contracts": "^2.0",
    "symfony/framework-bundle": "^4.4|^5.0",
    "symfony/dependency-injection": "^4.4|^5.0",
    "symfony/http-kernel": "^4.4|^5.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "symfony/phpunit-bridge": "^5.0",
    "symfony/translation": "^5.0",
    "symfony/yaml": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "PaLabs\\EnumBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PaLabs\\EnumBundle\\Test\\": "tests/"
    }
  }
}
