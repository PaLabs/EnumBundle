{
  "name": "palabs/enum-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle for php 8.1 enums",
  "keywords": ["enum", "php-enum", "symfony"],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "doctrine/common": "^2.8 || ^3.0 || ^4.0",
    "doctrine/orm": "^2.8 || ^3.0",
    "symfony/form": "^6.0 || ^7.0",
    "symfony/doctrine-bridge": "^6.0 || ^7.0",
    "symfony/translation-contracts": "^2.0 || ^3.0",
    "symfony/framework-bundle": "^6.0 || ^7.0",
    "symfony/dependency-injection": "^6.0 || ^7.0",
    "symfony/http-kernel": "^6.0 || ^7.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "symfony/phpunit-bridge": "^6.0 || ^7.0",
    "symfony/translation": "^6.0 || ^7.0",
    "symfony/yaml": "^6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "PaLabs\\EnumBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PaLabs\\EnumBundle\\Test\\": "tests/"
    }
  }
}
